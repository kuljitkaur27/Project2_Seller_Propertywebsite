<%= form_for @property do |f| %>
  <%= f.label :Location %>
  <%= f.text_field :location %>
  <%= f.label :Price %>
  <%= f.integer_field :price %>
  <%= f.label :Construction_Year %>
  <%= f.string_field :construction_year %>
  <%= f.label :Type %>
  <%= f.text_field :property_type%>
  <%= f.label :Bedrooms %>
  <%= f.integer_field :bedrooms%>
  <%= f.label :Bathrooms %>
  <%= f.integer_field :bathrooms%>
  <%= f.label :Car_Spaces %>
  <%= f.integer_field :car_spaces%>
  <%= f.label :Landsize %>
  <%= f.integer_field :landsize%>
  <%= f.label :Bonus_Features %>
  <%= f.text_field :bonus_features%>
  <%= f.label :Image %>
  <%= f.url_field :main_image %>
  <%= f.label :Image1 %>
  <%= f.url_field :image1 %>
  <%= f.label :Image2 %>
  <%= f.url_field :image2 %>
  <%= f.label :Image3 %>
  <%= f.url_field :image3 %>
  <%= f.submit %>
<% end %>
