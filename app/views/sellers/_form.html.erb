<%= form_for @seller do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :required => true, :autofocus => true %>
  <%= f.label :Phone %>
  <%= f.text_field :phone_no %>
  <%= f.label :Email %>
  <%= f.email_field :email %>
  <%= f.label :Password %>
  <%= f.text_field :password_digest %>

<%= f.submit %>
<% end %>
